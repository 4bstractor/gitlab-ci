- if @build.running?
  :plain
    $('#build-trace').html("#{escape_javascript(@build.trace_html)}");
    $('#build-trace').append('#{loader_html}');
    getBuild('#{project_build_path(@project, @build, bid: @build.id )}');
- else
  :plain
    location.href = "#{project_build_path(@project, @build)}";
